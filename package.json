{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "astro-base.mkz.dev",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maekzi/astro-base"
  },
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "pnpm run format:code",
    "format:code": "prettier --write **/*.{cjs,mjs,ts,md,mdx,json,astro,css,scss} --ignore-path --plugin=prettier-plugin-astro --plugin=prettier-plugin-tailwindcss",
    "lint": "eslint . --report-unused-disable-directives",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.9",
    "@astrojs/mdx": "^2.2.0",
    "@astrojs/react": "^3.1.0",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@iconify-json/mdi": "^1.1.64",
    "@types/react": "^18.2.66",
    "astro": "^4.5.5",
    "astro-compress": "^2.2.15",
    "astro-icon": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "focus-trap": "^7.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.6.0",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "sass": "^1.72.0",
    "svgo": "^3.2.0"
  }
}
