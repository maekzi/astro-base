{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "astro-base.mkz.dev",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maekzi/astro-base"
  },
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "pnpm run format:code",
    "format:code": "prettier --write **/*.{cjs,mjs,ts,md,mdx,json,astro,css,scss} --ignore-path --plugin=prettier-plugin-astro --plugin=prettier-plugin-tailwindcss",
    "lint": "eslint . --report-unused-disable-directives",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.1",
    "@iconify-json/mdi": "^1.1.66",
    "@types/react": "^18.3.3",
    "astro": "^4.15.9",
    "astro-compress": "^2.2.26",
    "astro-icon": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "focus-trap": "^7.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.0",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "sass": "^1.77.4",
    "svgo": "^3.3.2"
  }
}
