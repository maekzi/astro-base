{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "astro-base.mkz.dev",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maekzi/astro-base"
  },
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "pnpm run format:code",
    "format:code": "prettier --write **/*.{cjs,mjs,ts,md,mdx,json,astro,css,scss} --ignore-path --plugin=prettier-plugin-astro --plugin=prettier-plugin-tailwindcss",
    "lint": "eslint . --report-unused-disable-directives",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^5.1.1",
    "@iconify-json/mdi": "^1.2.0",
    "@types/react": "^18.3.11",
    "astro": "^4.15.11",
    "astro-compress": "^2.3.1",
    "astro-icon": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "focus-trap": "^7.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.2",
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.10.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "sass": "^1.79.4",
    "svgo": "^3.3.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
