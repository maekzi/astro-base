---
import { Image } from "astro:assets";

interface Props {
  class?: string;
  src: string;
  alt: string;
  width?: number;
  height?: number;
}

const { class: classNames, src, alt, width = 640, height = 360 } = Astro.props;
---

<Image class={classNames} src={src} alt={alt} width={width} height={height} inferSize format="webp" quality={80} />
